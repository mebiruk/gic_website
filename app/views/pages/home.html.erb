  <!-- ======= Contact Section ======= -->
  <section id="contact" class="contact">
    <div class="container" data-aos="fade-up">
      <div class="section-title">
        <h2>Contact</h2>
        <p>Magnam dolores commodi suscipit. Necessitatibus eius consequatur ex aliquid fuga eum quidem. Sit sint consectetur velit. Quisquam quos quisquam cupiditate. Et nemo qui impedit suscipit alias ea.</p>
      </div>
      <div class="text-center"><%= link_to "Check Our Branches", "/contact",class: "btn btn-primary mb-4"%></div>
      <div class="row" data-aos="fade-up" data-aos-delay="100">
        <div class="col-lg-6">
          <div class="row">
            <div class="col-md-12">
              <div class="info-box">
                <i class="bx bx-map"></i>
                <h3>Our Address</h3>
                <p>Ethiopia Addis Ababa, Asmara Road</p>
              </div>
            </div>
            <div class="col-md-6">
              <div class="info-box mt-4">
                <i class="bx bx-envelope"></i>
                <h3>Email Us</h3>
                <p>info@gic@edu.et<br>
                  contact@gic@edu.et</p>
              </div>
            </div>
            <div class="col-md-6">
              <div class="info-box mt-4">
                <i class="bx bx-phone-call"></i>
                <h3>Call Us</h3>
                <p>+251-11-6-460954<br>
                  +251-11-6-478965</p>
              </div>
            </div>
          </div>
        </div>
        <div class="col-lg-6">
          <%= simple_form_for(VisitorComment.new, url: visitor_comments_path, html: { class: 'php-email-form'}) do |f| %>
            <div class="form-row">
              <div class="col form-group">
                <%= f.input :fullname, class: "form-control"%>
                <div class="validate"></div>
              </div>
              <div class="col form-group">
                <%= f.input :category, collection: ["testimonial", "comment", "review"], class: "form-control"%>
                <div class="validate"></div>
              </div>
            </div>
            <div class="form-group">
              <%= f.input :subject, class: "form-control"%>
              <div class="validate"></div>
            </div>
            <div class="form-group">
              <%= f.input :message, class: "form-control", type: 'text'%>
              <div class="validate"></div>
            </div>
            <div class="mb-3">
              <div class="loading">Loading</div>
              <div class="error-message"></div>
              <div class="sent-message">Your message has been sent. Thank you!</div>
            </div>
            <%= f.submit 'Send Message', class: 'btn btn-primary text-white'%>
          <% end %>
        </div>
      </div>
    </div>
  </section>
  <!-- End Contact Section -->
</main><!-- End #main -->
