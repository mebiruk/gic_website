                      <%= image_tag pro.photo, class:"card-img-top rounded-0" %>
                      <div class="card-body d-flex flex-column">
                        <ul class="list-inline mb-2">
                          <li class="list-inline-item text-uppercase text-color"></i><%= pro.modality%></li>
                      </ul>
                      <%=link_to program_path(pro) do %>
                        <h4 class="card-title"><%=pro.program_name.html_safe%></h4>
                      <%end%>
                      <p class="card-text mb-1"><%=pro.overview.truncate(256).html_safe%></p>
                      <%= link_to 'Read More', program_path(pro), class:"btn btn-primary btn-sm" %>
                    </div>
                  </div>
                </div>
                <!-- course item -->
              <%end%>
              
              <!-- /course list -->
            </div>
              <!--<h5 class="text-center "><%= link_to 'More UnderGraduate Programs', programs_path %></h5>-->
              
          </div>
          <div>
            <h2 class="text-center mb-3">Our Graduate Programs</h2>
