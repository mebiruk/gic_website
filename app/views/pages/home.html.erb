<!-- events -->
<section class="section bg-gray">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="d-flex align-items-center section-title justify-content-between">
          <h2 class="mb-0 text-nowrap mr-3">Latest News and Events</h2>
          <div class="border-top w-100 border-primary d-none d-sm-block"></div>
          <div>
            <%= link_to 'see all', news_index_path, class:"btn btn-sm btn-primary-outline ml-sm-3 d-none d-sm-block" %>
          </div>
        </div>
      </div>
    </div>
    <div class="row justify-content-center">
      <!-- event -->
      <%@news.each do |news|%>
        <article class="col-lg-4 col-sm-6 mb-5 mb-lg-0  d-flex align-self-stretch">
          <div class="card rounded-0 border-bottom border-primary border-top-0 border-left-0 border-right-0 hover-shadow">
            <%= image_tag news.photo, class:"card-img-top rounded-0" %>
            <div class="card-body d-flex flex-column">
              <!-- post meta -->
              <ul class="list-inline mb-3">
                <!-- post date -->
                <li class="list-inline-item mr-3 ml-0"><%= Date.parse(news.created_at.to_s).strftime('%A')%>, <%= Date.parse(news.created_at.to_s).to_formatted_s(:long)%></li>
                <!-- author -->
                <li class="list-inline-item mr-3 ml-0">By <%= news.published_by%></li>
              </ul>
              <%= link_to news_path(news) do%>
                <h4 class="card-title"><%= news.news_title.html_safe%></h4>
              <% end%>
              <p class="card-text"><%= news.overview.truncate(256).html_safe%></p>
              <%= link_to 'Read More', news_path(news), class: "btn btn-primary btn-sm" %>
            </div>
          </div>
        </article>
      <%end%>
      <!-- event -->
      <!-- mobile see all button -->
      <div class="row">
        <div class="col-12 text-center">
          <a href="course.html" class="btn btn-sm btn-primary-outline d-sm-none d-inline-block">sell all</a>
        </div>
      </div>
    </div>
  </section>
  <!-- /events -->
