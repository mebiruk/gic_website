  <section class="panel theme--white" data-type="section" id="academics" data-modular-content="" data-js="panel" data-index="12">
    <header>
      <h2 class="center">Academics</h2>
      <p>Preparing students to make meaningful contributions&nbsp;to society as engaged citizens and leaders in a complex world</p>
    </header>
    <section class="panel" data-type="highlights" id="academics-highilights" data-width="full" data-posts-per-row="3">
      <div class="grid-container">
        <section class="highlight">
          <figure class="landscape">
            <picture>
              <!--[if IE 9]>
                <video style="display: none;"><![endif]-->
              <!--[if IE 9]></video>
              <![endif]-->
              <%= image_tag 'undrgraduate.jpg' %>
            </picture>
          </figure>
          <h3>Undergraduate Education</h3>
          <p>Rich learning experiences that provide a broad liberal arts foundation and deep subject-area expertise</p>
        </section>
        <section class="highlight">
          <figure class="landscape">
            <picture>
              <!--[if IE 9]>
                <video style="display: none;"><![endif]-->
              <%= image_tag 'graduated.jpg' %>
            </picture>
          </figure>
          <h3>Graduate Education</h3>
          <p>Unsurpassed opportunities to participate in the advancement of entire fields of knowledge</p>
        </section>
        <section class="highlight">
          <figure class="landscape">
            <picture>
              <%= image_tag 'training.jpg' %>
            </picture>
          </figure>
          <h3>Training</h3>
